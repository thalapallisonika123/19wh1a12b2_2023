#include <stdio.h>

int main(void) {
	int n;
	scanf("%d",&n);
	int i,j;
	for(i=0;i<n;i++)
	{
		for(j=0;j<n;j++)
		{
			if(j==0||j==(n-1))
			printf("*");
			else if(n/2==i)
			printf("*");
			else if(i==1&&j==1)
			printf("*");
			else if(i==3&&j==3)
			printf("*");
			else if(i==3&&j==1)
			printf("*");
			else if(i==1&&j==3)
			printf("*");
			else
			printf(" ");
		}
		printf("\n");
	}
	// your code goes here
	return 0;
}

